pipeline{
    agent none
    stages{
	  stage('Checkout code') {
	   agent {
	   label 'BuildServer'
	   }
	    steps{
	        checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'Github', url: 'https://github.com/gaddam146/Maven-Java-Project.git']]])
        }
      }
   }
}
